        },
        "translations": {
          "English": {
            "W. H. McLeod": {
              "translation": "The sight and touch of the Khalsa confers liberation and truth on the soul. 535 ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "PT6C",
        "source_page": 3,
        "source_line": 15,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
