        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The people will follow the vicious doctrines under the impact of great greed and attachment",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਵੀ ਬਿਨਾ ਕਿਸੇ ਮਰਯਾਦਾ ਬੁਰੀ ਚਾਲ ਚਲਣਗੇ ਅਤੇ ਲੋਭ ਮੋਹ ਦੇ ਬਹੁਤ ਵਸ ਵਿਚ ਹੋਣਗੇ।",
              "additional_information": {}
            }
          }
        }
