        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Were you not afraid of this group of brave-men (watchmen)?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਨ੍ਹਾਂ ਸੂਰਮਿਆਂ ਦੀ ਭੀੜ ਨੂੰ ਵੇਖ ਕੇ ਵੀ ਤੇਰੇ (ਮਨ ਵਿਚ) ਡਰ ਪੈਦਾ ਨਹੀਂ ਹੋਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
