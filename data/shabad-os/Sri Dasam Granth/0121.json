        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The intensely glorious Chandi killed the warriors and threw them in the field.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪ੍ਰਚੰਡ ਚੰਡੀ ਨੇ (ਰਾਖਸ਼) ਸੂਰਮਿਆਂ ਨੂੰ ਮਾਰ ਕੇ ਸਥਰ ਲਾਹ ਦਿੱਤੇ ਸਨ,",
              "additional_information": {}
            }
          }
        }
      },
