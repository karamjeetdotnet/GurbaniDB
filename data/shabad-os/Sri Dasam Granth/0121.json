        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "It appeared that the lightning had eradicated the minarets and thrown them headlong.9.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮਾਨੋ ਬਿਜਲੀ ਦੇ ਮਾਰੇ ਹੋਏ ਮੁਨਾਰੇ ਸਿਰ ਭਾਰ (ਡਿਗੇ ਪਏ) ਹੋਣ ॥੯॥",
              "additional_information": {}
            }
          }
        }
      }
