          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਆਕੀ ਹੋਏ ('ਮਲੰਦੇ') ਮਹਿਖਾਸੁਰ ਦੇ ਪੇਟ ਵਿਚ ਮਾਰ ਦਿੱਤੀ",
              "additional_information": {}
            }
          }
