          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(The sword) pierced the kindneys, intestines and the ribs.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਜੋ) ਗੁਰਦੇ, ਆਂਦਰਾਂ ਅਤੇ ਪਸਲੀਆਂ ('ਰੁਕੜੇ') ਨੂੰ ਖਾਂਦੀ (ਚੀਰਦੀ) ਹੋਈ (ਦੂਜੇ ਪਾਸੇ ਨਿਕਲ ਗਈ)।",
              "additional_information": {}
            }
          }
        }
