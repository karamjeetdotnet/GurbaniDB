        "id": "9ZP7",
        "source_page": 879,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "qw ky Dwm inqÎ cil AwvY ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦੇ ਘਰ ਨਿੱਤ ਚਲ ਕੇ ਆਉਂਦਾ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "VVZY",
        "source_page": 879,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "pRIiq Twin nih kyl kmwvY ]4]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He would come to her house every day, would show fondness but would not revel in making love.(4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪਰੰਤੂ ਉਸ ਨਾਲ ਪ੍ਰੇਮ ਪੂਰਵਕ ਰਤੀ-ਕ੍ਰੀੜਾ ਨਾ ਕਰਦਾ ॥੪॥",
              "additional_information": {}
            }
          }
        }
      }
