        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰੂਪ ਮਤੀ ਉਸ ਦੀ ਸੁੰਦਰ ਨਾਰੀ ਸੀ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "C7E6",
        "source_page": 879,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
