        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "In a country in the North, there lived a Raja who belonged to Sun clan.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੋ ਸੂਰਜ ਬੰਸ ਵਿਚ ਬਹੁਤ ਉਘੜਿਆ ਹੋਇਆ ਸੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "7JU3",
        "source_page": 879,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
