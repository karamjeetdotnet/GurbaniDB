        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "While revelling’ with other women he totally disregarded her affections.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਇਸ ਇਸਤਰੀ ਨਾਲ ਪ੍ਰੇਮ ਕਰਨਾ ਭੁਲਾ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
