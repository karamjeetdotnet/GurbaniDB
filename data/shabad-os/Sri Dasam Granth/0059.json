        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qy pRBu jn kCu kY n ibcwrY ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Those persons who adopt different guises are never liked by the men of God.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਨ੍ਹਾਂ (ਭੇਖਧਾਰੀਆਂ ਨੂੰ) ਸੱਚੇ ਸਾਧਕ (ਪ੍ਰਭੂ-ਜਨ) ਕੁਝ ਵੀ ਨਹੀਂ ਸਮਝਦੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "NCNA",
        "source_page": 59,
        "source_line": null,
