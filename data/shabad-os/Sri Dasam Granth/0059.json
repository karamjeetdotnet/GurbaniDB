          "SGPC": "pwrbRhm ko iqnI pCwnw ]55]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Only those who controlled their minds, recognized the Supreme Brahman.55.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਨ੍ਹਾਂ ਨੇ ਹੀ ਬ੍ਰਹਮ ਨੂੰ ਪਛਾਣਿਆ ਹੈ ॥੫੫॥",
              "additional_information": {}
            }
          }
        }
      }
