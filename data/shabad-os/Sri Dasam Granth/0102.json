        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "igrY bIr ipMgwC dybI sMGwry ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When the warriors like Pingachh fell in the field,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਜਦੋਂ) ਦੇਵੀ ਦੇ ਮਾਰੇ ਪਿੰਗਾਛ (ਜਿਹੇ) ਯੋਧੇ ਡਿਗੇ,",
              "additional_information": {}
            }
          }
        }
      },
