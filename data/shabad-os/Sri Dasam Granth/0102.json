        "source_page": 102,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qbY rois dyibXM sroGM clwey ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then the goddess in great ire shot many arrows,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਦੇਵੀ ਨੇ ਕ੍ਰੋਧਵਾਨ ਹੋ ਕੇ ਬਹੁਤ ਸਾਰੇ ਤੀਰ (ਸਰ-ਓਘ) ਚਲਾਏ (ਅਤੇ ਸੂਰਬੀਰਾਂ ਨੂੰ)",
              "additional_information": {}
            }
          }
        }
      },
