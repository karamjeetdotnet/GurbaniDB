        "source_page": 102,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "ibnw pRwn ky juD mDM igrwey ]25]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Which laid to rest many warriors in the battlefield.25.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪ੍ਰਾਣਾਂ ਤੋਂ ਸਖਣਾ ਕਰ ਕੇ ਯੁੱਧ-ਭੂਮੀ ਵਿਚ ਡਿਗਾ ਦਿੱਤਾ ॥੨੫॥",
              "additional_information": {}
            }
          }
        }
      }
