        "source_page": 102,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "cly Aauru bIrM hiQAwrM auGwry ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Other warriors holding their weapons marched forward.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਹੋਰ ਯੋਧੇ ਸ਼ਸਤ੍ਰ ਉਲ੍ਹਾਰ ਕੇ (ਯੁੱਧ-ਭੂਮੀ ਵਲ) ਚਲ ਪਏ।",
              "additional_information": {}
            }
          }
        }
      },
      {
