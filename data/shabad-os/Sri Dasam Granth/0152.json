    "lines": [
      {
        "id": "VW5S",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Sirlekh",
        "gurmukhi": {
          "SGPC": "cOpeI ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "CHAUPAI",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚੌਪਈ:",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JP65",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qb BUpq qh dUq pTwey ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The king (Ajai Singh) sent his messengers (to king Tilak) and (Sanaudhi Brahmin).",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਰਾਜੇ (ਅਜੈ ਸਿੰਘ) ਨੇ ਉਥੇ ਦੂਤ ਭੇਜ ਦਿੱਤੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "67G5",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "iqRpq skl idj kIey irJwey ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Who satisfied all the incoming Brahmin.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਸਨੌਢੀਏ) ਬ੍ਰਾਹਮਣ ਨੇ ਸਾਰਿਆਂ ਨੂੰ ਚੰਗੀ ਤਰ੍ਹਾਂ ਰਜਾ ਕੇ ਤ੍ਰਿਪਤ ਕੀਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "WZZ0",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "AsmyD Aru Asumyd hwrw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(These messengers saidJ “Asumedh and Asumedhan,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਇਸ ਪਿਛੋਂ ਉਨ੍ਹਾਂ ਦੂਤਾਂ ਨੇ ਬੇਨਤੀ ਕੀਤੀ ਕਿ) ਅਸੁਮੇਧ ਅਤੇ ਅਸੁਮੇਦ ਹਾਰ ਖਾ ਕੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "ZPB4",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "Bwj pry Gr qwk iqhwrw ]1]287]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Have run and hid themselves in thy home.1.287.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਭਜੇ ਹੋਏ ਤੁਹਾਡੇ ਘਰ ਆਏ ਹਨ ॥੧॥੨੮੭॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "GSFJ",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "kY idj bwD dyhu duAY mohU ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“O Brahmin, either bind and deliver them to us",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਾਂ ਤਾਂ ਪੰਡਿਤ ਜੀ! ਦੋਵੇਂ ਬੰਨ੍ਹ ਕੇ ਸਾਨੂੰ ਦੇ ਦਿਓ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "N40N",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "nwqr Dro dujnvw qohU ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“O thou shalt be considered like them",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨਹੀਂ ਤਾਂ ਤੁਹਾਨੂੰ ਵੀ ਉਨ੍ਹਾਂ ਦੋਹਾਂ ਨਾਲ ਮਿਲਾ ਦੇਵਾਂਗੇ (ਮਾਰ ਦੇਵਾਂਗੇ)।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "HPD2",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "kirE n pUjw dyau n dwnw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Thou shalt neither be worshipped nor any gift be given to thee",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਫਿਰ) ਨਾ ਆਪ ਨੂੰ ਦਾਨ ਦਿੱਤਾ ਜਾਵੇਗਾ ਅਤੇ ਨਾ ਹੀ ਪੂਜਾ ਕੀਤੀ ਜਾਵੇਗੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JAFA",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qo ko duK dyvo idj nwnw ]2]288]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Thou shalt then be given various types of sufferings.2.288.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਸਗੋਂ) ਤੁਹਾਨੂੰ ਕਈ ਪ੍ਰਕਾਰ ਦੇ ਦੁਖ ਦਿੱਤੇ ਜਾਣਗੇ ॥੨॥੨੮੮॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "A9Z1",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "khw imRqk duie kMT lgwey ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Why hast thou hugged these two dead ones to thy bosom?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕੀ ਦੋਵੇਂ ਮੁਰਦੇ (ਤੁਸੀਂ) ਗੱਲ ਨਾਲ ਲਾ ਲਏ ਹਨ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "1HZ5",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "dyhu hmY qum khw ljwey ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Give them back to us  Why art thou hesitating?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸਾਨੂੰ ਦੇ ਦੇਣ ਵਿਚ ਤੁਹਾਨੂੰ ਕੀ ਸੰਕੋਚ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "008Q",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "jau duAY ey qum dyhu n mohU ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“If thou dost not return both of them to me,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੇ ਇਹ ਦੋਵੇਂ ਸਾਨੂੰ ਨਹੀਂ ਦੇਓਗੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "R7ZW",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qau hm isK n hoie hY qohU ]3]289]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Then we shall not be thy disciples.”3.289.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਅਸੀਂ ਤੁਹਾਡੇ ਸਿੱਖ (ਸੇਵਕ) ਨਹੀਂ ਹੋਵਾਂਗੇ ॥੩॥੨੮੯॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "L5A9",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qb idj pRwq kIE iesnwnw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then the Sanaudhi Brahmin got up early in the morning and took bath.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਪੰਡਿਤ ਸਵੇਰੇ ਜਵਾਬ ਦੇਣ ਲਈ ਆਗਿਆ ਲੈ ਕੇ ਘਰ ਚਲਾ ਗਿਆ) ਤਦ ਸਵੇਰੇ ਬ੍ਰਾਹਮਣ ਨੇ ਇਸ਼ਨਾਨ ਕੀਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "F84V",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "dyv ipqR qoKy ibD nwnw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He worshipped in various ways the gods and manes.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦੇਵਤਿਆਂ ਅਤੇ ਪਿਤਰਾਂ ਨੂੰ ਕਈ ਤਰ੍ਹਾਂ ਨਾਲ ਪ੍ਰਸੰਨ ਕੀਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "H6CZ",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "cMdn kuMkm Kor lgwey ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then he put the frontal marks of sandal and saffron on his forehead.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਫਿਰ ਮੱਥੇ ਤੇ) ਚੰਦਨ ਅਤੇ ਕੇਸਰ ਦੇ ਟਿਕੇ ਲਗਾਏ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "R1P3",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "cl kr rwj sBw mY Awey ]4]290]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After that he walked upto his court.4.290.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਚਲ ਕੇ ਰਾਜ-ਸਭਾ ਵਿਚ ਆ ਗਿਆ ॥੪॥੨੯੦॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
  {
    "id": "894",
    "sttm_id": null,
