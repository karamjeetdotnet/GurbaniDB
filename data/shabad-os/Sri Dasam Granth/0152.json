    "lines": [
      {
        "id": "KVMC",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "idjo bwc ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The Brahmin said:",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬ੍ਰਾਹਮਣ ਨੇ ਕਿਹਾ:",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "81HU",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "hmrI vY n prY duAY fITw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Neither have I seen both of them,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹ ਦੋਵੇਂ ਮੇਰੀ ਨਜ਼ਰ ਵਿਚ ਨਹੀਂ ਪਏ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "TY5W",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "hmrI Awie prY nhI pITw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Nor have they taken shelter.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨਾ ਹੀ ਮੇਰੀ ਸ਼ਰਨ ਵਿਚ ਆ ਕੇ ਪਏ ਹਨ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "3T5J",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "JUT kihXo ijn qoih sunweI ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Whosoever has given thee news about them, he hath told a lie,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਨੇ ਤੁਹਾਨੂੰ (ਇਹ ਗੱਲ) ਦਸੀ ਹੈ, ਉਸ ਨੇ ਝੂਠ ਕਿਹਾ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "C635",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "mhwrwj, rwjn ky rweI ]1]291]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“O Emperor, the king of kings.1.291.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਮਹਾਰਾਜ! ਤੁਸੀਂ ਰਾਜਿਆਂ ਦੇ ਰਾਜੇ ਹੋ ॥੧॥੨੯੧॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "LP6X",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "mhwrwj rwjn ky rwjw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“O Emperor, the king of kings,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਮਹਾਰਾਜ! ਰਾਜਿਆਂ ਦੇ ਰਾਜੇ!",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "1P1G",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "nwiek AKl Drx isr qwjw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“O the hero of all the universe and Master of the earth",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਤੁਸੀਂ) ਸਿਰ ਉਤੇ ਤਾਜ ਰਖਣ ਵਾਲੇ ਸਾਰਿਆਂ ਦੇ ਸੁਆਮੀ ਹੋ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "50FH",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "hm bYTy qum dyhu AsIsw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“While sitting here, I am giving blessings to thee,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਸੀਂ (ਇਥੇ) ਬੈਠੇ ਤੁਹਾਨੂੰ ਅਸੀਸਾਂ ਦੇ ਰਹੇ ਹਾਂ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "CCGU",
        "source_page": 152,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qum rwjw rwjn ky eIsw ]2]292]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“Thou , O monarch, art the Lord of the kings.”2.292.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਤੁਸੀਂ ਰਾਜਿਆਂ ਦੇ ਸੁਆਮੀ ਰਾਜੇ (ਬਣੇ ਰਹੋ) ॥੨॥੨੯੨॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
