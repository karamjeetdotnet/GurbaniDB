        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Balram said to Krishna, “This warrior is fighting for a very long time",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬਲਰਾਮ ਨੇ ਕ੍ਰਿਸ਼ਨ ਨੂੰ ਕਿਹਾ ਕਿ ਵੈਰੀ ਸੂਰਮਾ ਰਣ ਵਿਚ ਬਹੁਤ ਲੜਿਆ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
