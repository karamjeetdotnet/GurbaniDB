        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "With her left hand, she caused the dance of the lionss of steel (sword).",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਦੇਵੀ ਨੇ) ਖਬੇ ਹੱਥ ਵਿਚ ਲੋਹੇ ਦੀ ਸ਼ੇਰਨੀ (ਤਲਵਾਰ) ਨਚਾਈ",
              "additional_information": {}
            }
          }
        }
