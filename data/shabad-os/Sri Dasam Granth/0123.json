        "source_page": 123,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "jg ivc vfw dwno, Awp khwieAw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He caused himself to be called the great demon.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਜੋ) ਜਗਤ ਵਿਚ ਆਪਣੇ ਆਪ ਨੂੰ ਵੱਡਾ ਦੈਂਤ ਅਖਵਾਉਂਦਾ ਸੀ।",
              "additional_information": {}
            }
          }
        }
      },
