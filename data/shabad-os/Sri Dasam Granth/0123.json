        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She pulled her double-edged sword from her scabbard and came before the enemy.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮਿਆਨ ਵਿਚੋਂ ਖੰਡਾ ਖਿਚ ਕੇ (ਦੈਂਤ ਦੇ) ਸਾਹਮਣੇ ਹੋ ਗਈ।",
              "additional_information": {}
            }
          }
        }
      },
