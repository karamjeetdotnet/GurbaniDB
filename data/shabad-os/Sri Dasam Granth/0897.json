          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨਾਲ ਭਾਂਤ ਭਾਂਤ ਦੇ ਆਸਣ ਦਿੱਤੇ",
              "additional_information": {}
