          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸ਼ਾਹ ਨੇ ਦੋ ਸੌ ਗਜ਼ ਦੇ (ਬਾਂਸ ਦੇ) ਦੋ ਝੰਡੇ ਮੰਗਾ ਲਏ।",
              "additional_information": {}
            }
          }
        }
      },
