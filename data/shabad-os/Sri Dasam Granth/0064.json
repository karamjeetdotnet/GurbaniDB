        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "iqnY Kyd kY bwir ky bIc fwrM ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then the Lord brought the end of the fight and the enemy was driven out into the river.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਉਨ੍ਹਾਂ (ਵੈਰੀਆਂ) ਨੂੰ ਖਦੇੜ ਕੇ (ਨਦੀ ਦੇ) ਜਲ ਵਿਚ ਸੁਟ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
