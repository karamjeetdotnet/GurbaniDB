        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "I then threw away the gun and took the arrows in my hand, I shot four of them.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚਾਰ ਬਾਣ ਲੈ ਕੇ ਸਜੇ ਹੱਥ ਨਾਲ ਛਡੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
