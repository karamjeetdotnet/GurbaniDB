        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qbY Coirgy bwr dyvY dmwmM ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Sometime after midnight they left, while beating the drums.",
              "additional_information": {}
            }
          },
