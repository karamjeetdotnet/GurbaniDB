        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "But after a few breaths he would become unconscious again.(4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦ ਸੁਆਸ ਹੇਠਾਂ ਨੂੰ ਚਲਦਾ ਤਾਂ ਕੁਝ ਸੁੱਧ ਬੁੱਧ ਨਾ ਰਹਿੰਦੀ ॥੪॥",
              "additional_information": {}
            }
          }
        }
