        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "One day Raja, when he was proceeding to her house, he saw him.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਕ ਦਿਨ ਰਾਜੇ ਨੇ (ਚੋਰ ਨੂੰ) ਘਰ ਆਉਂਦਿਆਂ ਵੇਖ ਲਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
