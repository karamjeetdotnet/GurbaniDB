            "Dr. Surinder Singh Kohli": {
              "translation": "She brought a big basket and asked them both to sit in there.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਦੋਹਾਂ ਨੂੰ ਦੋਹਾਂ ਸਿਰਿਆਂ ਉਤੇ ਬਿਠਾ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
