          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਦੇ ਮਾਂ ਪਿਓ ਦਾ ਪਤਾ ਨਹੀਂ,",
              "additional_information": {}
            }
          }
        }
