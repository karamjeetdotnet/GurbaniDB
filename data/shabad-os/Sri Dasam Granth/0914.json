        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਕ ਪਿਟਾਰਾ ਹੋਰ ਮੰਗਵਾਇਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "VZK9",
        "source_page": 914,
        "source_line": null,
        "pronunciation": null,
