        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Who has no father or mother, why did she marry the daughter to him?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ ਕਿਸ ਲਈ ਆਪਣੀ ਲੜਕੀ ਵਿਆਹ ਰਹੀ ਹੈਂ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "E7ST",
        "source_page": 914,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "Xw kO AbY bwiD kir mwro ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸ (ਜੋਗੀ) ਨੂੰ ਹੁਣ ਬੰਨ੍ਹ ਕੇ ਮਾਰ ਦਿਓ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "5KRC",
        "source_page": 914,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "rwnI duhqw sihq sMGwro ]7]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Now tie him, kill him and also terminate the Rani and the daughter.’(7)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਰਾਣੀ ਨੂੰ ਲੜਕੀ ਸਹਿਤ ਖ਼ਤਮ ਕਰ ਦਿਓ ॥੭॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "N5M1",
        "source_page": 914,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "rwnI bcn sunq fir geI ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਣੀ ਇਹ ਬੋਲ ਸੁਣ ਕੇ ਡਰ ਗਈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "Q70S",
        "source_page": 914,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
