        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ (ਉਪਾ) ਨਾਲ ਜਵਾਈ ਨੂੰ ਨਾ ਮਾਰਿਆ ਜਾਏ",
              "additional_information": {}
            }
          }
        }
      },
