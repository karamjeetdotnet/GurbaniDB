            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਰਾਜੇ ਤੋਂ ਇਹ ਜੋਗੀ ਲੈ ਲੈਣਾ ਚਾਹੀਦਾ ਹੈ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "7QY2",
        "source_page": 914,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "Byij mwnuKn Xw kO gihXY ]4]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "That she would ask Raja to fetch the mendicant.(4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਬੰਦੇ ਭੇਜ ਕੇ ਇਸ ਨੂੰ ਪਕੜਵਾ ਲੈਣਾ ਚਾਹੀਦਾ ਹੈ ॥੪॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
