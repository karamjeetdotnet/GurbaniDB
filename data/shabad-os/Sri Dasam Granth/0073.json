        "source_page": 73,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "CÍY n skY iqh Cwih kO inhPl jwie gvwr ]24]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "None can touch his shadow, the fool makes useless effort.24.",
              "additional_information": {}
            }
