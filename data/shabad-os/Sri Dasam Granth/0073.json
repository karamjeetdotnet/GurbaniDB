        "source_page": 73,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "jo swDU srxI pry iqn ky kvn ibcwr ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Those who have taken refuge with the saints, what can be said about them?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਹੜੇ ਸੰਤਾਂ ਦੀ ਸ਼ਰਨ ਵਿਚ ਪਏ ਹਨ, ਉਨ੍ਹਾਂ ਦਾ ਕੀ ਵਿਚਾਰ ਕਹੀਏ?",
              "additional_information": {}
            }
          }
        }
      },
