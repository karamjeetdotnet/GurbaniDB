        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The king and also his subject performed evil acts",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਜਾ ਅਤੇ ਪ੍ਰਜਾ ਸਭ ਜਿਥੇ ਕਿਥੇ ਕੁਕਰਮ ਕਰਨ ਲਗ ਗਏ ਹਨ।",
              "additional_information": {}
            }
          }
        }
      },
