        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Several new paths were initiated and adharma increased in the world",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਗਤ ਵਿਚ ਨਵੇਂ ਨਵੇਂ ਮਾਰਗ (ਪੰਥ) ਚਲ ਪਏ ਹਨ ਅਤੇ ਅਧਰਮ ਵਧ ਗਿਆ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
