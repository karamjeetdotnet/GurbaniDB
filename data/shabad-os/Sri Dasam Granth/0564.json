        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": ", The dharma was destroyed and the sinful activities were extended.135.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਧਰਮ ਖੰਭ ਲਾ ਕੇ ਉਡ ਗਿਆ ਹੈ ਅਤੇ ਪਾਪ ਦੀ ਕ੍ਰਿਆ ਪਸਾਰ ਦਿੱਤੀ ਹੈ ॥੧੩੫॥",
              "additional_information": {}
            }
          }
        }
      },
