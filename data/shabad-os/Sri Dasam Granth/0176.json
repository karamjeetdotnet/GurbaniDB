          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਛਤਰੀਆਂ ਨੂੰ ਮਾਰ ਕੇ (ਪਰਸ਼ੁਰਾਮ ਨੇ) ਧਰਤੀ ਖੋਹ ਲਈ।",
              "additional_information": {}
            }
          }
        }
