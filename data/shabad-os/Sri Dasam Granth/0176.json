          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅੰਤ ਵਿਚ ਅਚੇਤ ਹੋ ਕੇ ਰਾਜਾ ਯੁੱਧ-ਭੂਮੀ ਵਿਚ ਪਿਆ ਰਿਹਾ।",
              "additional_information": {}
            }
          }
        }
