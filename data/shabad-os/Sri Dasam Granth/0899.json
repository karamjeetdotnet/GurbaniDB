        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "mugl mhInw kY qurq; cwkr kIno qws ]3]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The Mughal immediately engaged him on monthly terms.(3)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੁਗ਼ਲ ਨੇ ਉਸ ਨੂੰ ਤੁਰਤ ਮਹੀਨੇ ਦੀ (ਬੰਨ੍ਹੀ ਨੌਕਰੀ) ਤੇ ਨੌਕਰ ਰਖ ਲਿਆ ॥੩॥",
              "additional_information": {}
            }
          }
        }
      }
