        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘If you had borrowed money from some one, how could he steal from you?",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
