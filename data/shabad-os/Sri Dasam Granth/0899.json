        "id": "XVV7",
        "source_page": 899,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "skl lok Ttky rhY; rYnweI liK pwie ]5]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He put the people into astonishment, took the horse and went away.(5)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸਭ ਲੋਗ (ਉਸ ਨੂੰ) ਕਰਜ਼ਾਈ ('ਰੈਨਾਈ') ਸਮਝ ਕੇ ਹੈਰਾਨ ਰਹਿ ਗਏ ॥੫॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
