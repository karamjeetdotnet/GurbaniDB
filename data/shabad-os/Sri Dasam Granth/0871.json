        "gurmukhi": {
          "Budha Dal Mehron": "inkis Awpu TwFI BeI; jY duMdBI bjwie ]21]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And beating the drum, herself, she came and stood there.(21)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਆਪ ਨਿਕਲ ਕੇ ਡਟ ਗਈ ਅਤੇ ਜਿੱਤ ਦਾ ਨਗਾਰਾ ਵਜਾ ਦਿੱਤਾ ॥੨੧॥",
              "additional_information": {}
            }
          }
        }
      }
