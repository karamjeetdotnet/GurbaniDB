        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I will send all those invincible warriors to the hell today.’(20)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਜ ਮੈਂ ਵਿਕਟ ਅਤੇ ਸ੍ਰੇਸ਼ਠ ਸੂਰਮਿਆਂ ਨੂੰ ਯਮਲੋਕ ਭੇਜ ਦਿਆਂਗੀ ॥੨੦॥",
              "additional_information": {}
            }
          }
        }
      },
      {
