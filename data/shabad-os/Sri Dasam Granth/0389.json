        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Wherever he used to sing, we also sang songs of praise with him",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹ ਉਸ ਥਾਂ ਉਤੇ ਗਾਉਂਦਾ ਹੁੰਦਾ ਸੀ ਅਤੇ ਅਸੀਂ ਮਿਲ ਕੇ ਉਥੇ ਮੰਗਲਮਈ ਗੀਤ ਗਾਏ ਸਨ।",
              "additional_information": {}
            }
          }
        }
      },
