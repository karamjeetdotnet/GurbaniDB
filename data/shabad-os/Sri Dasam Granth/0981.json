        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The handsome Raja was killed after challenges.(11)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸੁੰਦਰ ਰਾਜੇ ਨੂੰ ਲਲਕਾਰ ਲਲਕਾਰ ਕੇ ਮਾਰ ਦਿੱਤਾ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
