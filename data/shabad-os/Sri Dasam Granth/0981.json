            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾਂ ਉਸ ਤੁਰਕ ਨੇ ਕ੍ਰੋਧ ਨਾਲ ਵਾਰ ਕੀਤਾ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "USSS",
        "source_page": 981,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "bwhn duhUM duDw kir idXo ]10]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(As he could not,) Then the Turk struck and cut his both the arms.(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ (ਉਸ ਦੀਆਂ) ਦੋਵੇਂ ਬਾਂਹਵਾਂ ਦੋ ਦੋ ਟੋਟੇ ਕਰ ਦਿੱਤੀਆਂ ॥੧੦॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
