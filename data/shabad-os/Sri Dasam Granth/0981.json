        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Teer Khan, who was standing there, tied up his arms.(9)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਖ਼ਾਨ ਕੋਲ ਖੜੋਤਾ ਸੀ, ਉਸ ਨੇ ਮੁਸ਼ਕਾਂ ਕਸ ਦਿੱਤੀਆਂ ॥੯॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
