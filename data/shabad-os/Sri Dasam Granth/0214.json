        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Hearing these words of the enemy (Parashuram), Ram looked like a mighty hero.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਵੈਰੀ ਦੇ ਇਸ ਤਰ੍ਹਾਂ ਦੇ ਬੋਲ ਸੁਣੇ ਤਦੋਂ ਸ੍ਰੀ ਰਾਮ ਸੂਰਮੇ ਵਾਂਗ ਉਛਲ ਕੇ ਅੱਗੇ ਆਏ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "BCAY",
        "source_page": 215,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "swq smuMdRn lO grvy igr; BUim Akws doaU Qhrwny ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Visualising the serene posture of Ram, exhibiting the serenity of seven seas, the mountains, Sky and the whole world trembled.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸੱਤ ਸਮੁੰਦਰਾਂ ਤੱਕ ਪਹਾੜ ਦਬ ਗਏ ਅਤੇ ਧਰਤੀ ਤੇ ਆਕਾਸ਼ ਦੋਵੇਂ ਕੰਬਣ ਲੱਗ ਪਏ।",
              "additional_information": {}
            }
          }
        }
      },
