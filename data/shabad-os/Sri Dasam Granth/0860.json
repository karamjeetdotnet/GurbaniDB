          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦ ਸਭ ਨੇ ਕੂਕ ਕੇ ਕਿਹਾ, ਹੇ ਮੂਰਖ! ਇਸ ਨੂੰ ਘੁਟ ਕੇ ਪਕੜ ॥੧੫॥",
              "additional_information": {}
            }
          }
        }
