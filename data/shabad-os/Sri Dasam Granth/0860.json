          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਗਾਲ੍ਹਾਂ ਕਢਦੀ ਹੋਈ ਰੁੜ੍ਹ ਗਈ ਅਤੇ ਯਮ ਪੁਰੀ ਵਿਚ ਜਾ ਪਹੁੰਚੀ ॥੧੬॥",
              "additional_information": {}
            }
          }
