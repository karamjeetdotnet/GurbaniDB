          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕੁਲੱਛਣੀ ਔਰਤ ਨੂੰ ਡੋਬ ਕੇ ਜਟ ਘਰ ਆ ਗਿਆ।",
              "additional_information": {}
            }
          }
        }
