          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਉਸ ਨੇ ਕੂਕ ਕੰਨ ਨਾਲ ਸੁਣੀ ਤਾਂ ਹੱਥ ਵਿਚੋਂ ਪੂਛਲ ਛਡ ਦਿੱਤੀ",
              "additional_information": {}
            }
          }
        }
