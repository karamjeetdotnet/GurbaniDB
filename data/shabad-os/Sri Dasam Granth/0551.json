          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ (ਆਪਣੇ) ਪਿਤਾ ਇੰਦਰ ਦੇ ਦੁਖ ਖ਼ਤਮ ਕਰ ਦਿੱਤੇ।",
              "additional_information": {}
            }
          }
        }
