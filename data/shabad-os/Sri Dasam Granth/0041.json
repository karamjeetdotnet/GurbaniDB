        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Thou hast created the Svetaja, Jeraju and Uddahhujja division of creation. !",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਨੇ) ਸੇਤਜ, ਜੇਰਜ ਅਤੇ ਉਤਭੁਜ ਦੀ ਸਿਰਜਨਾ ਕੀਤੀ ਹੈ।",
              "additional_information": {}
            }
          }
        }
