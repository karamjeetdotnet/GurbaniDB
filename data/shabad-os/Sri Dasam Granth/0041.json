        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Thou hast also created the directions, the indivcations, the earth and the sky. !",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਨੇ ਹੀ) ਦਿਸ਼ਾ, ਵਿਦਿਸ਼ਾ, ਜ਼ਮੀਨ ਅਤੇ ਆਸਮਾਨ (ਬਣਾਏ ਹਨ)",
              "additional_information": {}
            }
          }
        }
