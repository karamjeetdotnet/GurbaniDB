        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "First she saw Balbhadar and then Krishna.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪਹਿਲਾਂ ਬਲਭਦ੍ਰ ('ਲਾਗਿਲੀ') ਨੂੰ ਵੇਖਿਆ, ਫਿਰ ਕ੍ਰਿਸ਼ਨ ਨੂੰ ਵੇਖਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
