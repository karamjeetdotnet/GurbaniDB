        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After creating there, fourteen fairies, she suggested her,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚੌਦਾਂ ਪੁਰੀਆਂ ਬਣਾ ਕੇ ਉਸ ਨੂੰ ਸਮਝਾ ਕੇ ਕਿਹਾ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "DQVX",
        "source_page": 1015,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
