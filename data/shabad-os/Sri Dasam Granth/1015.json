        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I have devised the means for your survival, come and see yourself.’(22)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਤੇਰੇ ਜੀਣ ਦਾ ਉਪਾ ਮੈਂ ਬਣਾਇਆ ਹੈ, ਆ ਕੇ ਵੇਖ ॥੨੨॥",
              "additional_information": {}
            }
