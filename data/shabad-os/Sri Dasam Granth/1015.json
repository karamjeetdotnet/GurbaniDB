        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "hÍY pRsMnÎ pwien prI jgq gurU Thrwie ]25]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She was appeased and, believing them to be the gurus of the world, bent and paid her obeisance.(25)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਫਿਰ (ਉਸ ਨੂੰ) ਜਗਤ ਗੁਰੂ ਮਿਥ ਕੇ ਝੁਕ ਕੇ ਪੈਰੀਂ ਪਈ ॥੨੫॥",
              "additional_information": {}
            }
          }
        }
      }
