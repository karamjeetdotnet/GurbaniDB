        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Where all the members of Jadav family, including Krishna, were sitting.(24)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਥੇ ਸਾਰੇ ਖ਼ਾਨਦਾਨ ਸਹਿਤ ਸ੍ਰੀ ਕ੍ਰਿਸ਼ਨ ਬਿਰਾਜਮਾਨ ਸਨ ॥੨੪॥",
              "additional_information": {}
            }
          }
        }
      },
