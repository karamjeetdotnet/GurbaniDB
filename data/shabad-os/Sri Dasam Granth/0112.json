          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚੁੜੇਲਾਂ ਬੋਲ ਰਹੀਆਂ ਸਨ ਅਤੇ ਡਾਕਣੀਆਂ ਡਕਾਰ ਰਹੀਆਂ ਸਨ ॥੩॥੧੫੯॥",
              "additional_information": {}
            }
          }
        }
