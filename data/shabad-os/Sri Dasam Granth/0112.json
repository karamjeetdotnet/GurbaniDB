          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਵੱਡੀਆਂ ਗਿੱਧਾਂ ਉਡ ਰਹੀਆਂ ਸਨ ਅਤੇ ਭਿਆਨਕ ਕਾਂ ਸ਼ੋਰ ਪਾ ਰਹੇ ਸਨ।",
              "additional_information": {}
            }
          }
        }
