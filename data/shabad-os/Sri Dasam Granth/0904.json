          "Budha Dal Mehron": "qw ky kwl inkt nih AwvY ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Those who recognise Satnam, the angel of death comes not near them,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦੇ ਨੇੜੇ ਕਾਲ ਨਹੀਂ ਆ ਸਕਦਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "4BWL",
        "source_page": 904,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
