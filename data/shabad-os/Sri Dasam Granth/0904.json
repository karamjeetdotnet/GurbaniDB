        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘The old and the young, all should meditate on him,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਦੇ ਨਾਮ ਨੂੰ ਜਪੇ ਬਿਨਾ ਕੋਈ ਬਾਲ ਅਥਵਾ ਬਿਰਧ ਨਹੀਂ ਰਹਿ ਸਕਦਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "BTQR",
        "source_page": 904,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "rwv rMk rwjw, sBY; ijXq n rhsI koie ]20]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘(Without his name) the rulers or the subject, nothing will remain.(20)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਰਾਜਨ! ਸਾਰੇ ਰਾਓ ਅਤੇ ਰੰਕ ਜੀਉਂਦੇ ਨਹੀਂ ਰਹਿਣਗੇ ॥੨੦॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
