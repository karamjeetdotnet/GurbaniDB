        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Those who accept Satnam, Satnam prevails in their eloquence.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਸਤਿਨਾਮ (ਨੂੰ ਗੁਰਾਂ) ਦੇ ਬਚਨਾਂ ਦੁਆਰਾ ਪ੍ਰਮਾਣਿਕ ਮੰਨਦਾ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "4MVY",
        "source_page": 904,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
