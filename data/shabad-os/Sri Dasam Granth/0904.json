        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘The sky and the earth are like two grinding stones.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਆਕਾਸ਼ ਅਤੇ ਧਰਤੀ ਦੋਵੇਂ ਚੱਕੀ ਦੇ ਪੁੜਾਂ ਵਰਗੇ ਬਣੇ ਹੋਏ ਹਨ।",
              "additional_information": {}
            }
