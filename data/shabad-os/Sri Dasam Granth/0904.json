        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸਤਿਨਾਮ ਲੈ ਕੇ ਮਾਰਗ ਉਤੇ ਚਲਦਾ ਹੈ,",
