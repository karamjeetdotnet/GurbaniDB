        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Coming any thing in between is saved not.(22)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਨ੍ਹਾਂ ਦੋਹਾਂ ਪੁੜਾਂ ਵਿਚ ਆਣ ਵਾਲਾ ਸਾਬਤ ਨਹੀਂ ਬਚਿਆ ਹੈ ॥੨੨॥",
              "additional_information": {}
            }
          }
        }
      }
