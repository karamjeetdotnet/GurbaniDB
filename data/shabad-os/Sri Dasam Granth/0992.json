        "id": "UKQN",
        "source_page": 992,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "DMn rwnI, qY mwir Air; mwir sqRü piq lIn ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Rani, you are admirable, you defeated the enemy and its Raja too.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਰਾਣੀ! ਤੂੰ ਧੰਨ ਹੈਂ, ਤੂੰ ਵੈਰੀ ਨੂੰ ਮਾਰ ਕੇ (ਫਿਰ) ਵੈਰੀ-ਨਾਇਕ ਨੂੰ ਵੀ ਮਾਰ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
