        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘In all the fourteen worlds, there has never been and never will be a woman like you.(32)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਜ ਤਕ ਚੌਦਾਂ ਲੋਕਾਂ ਵਿਚ ਤੇਰੇ ਵਰਗੀ ਕੋਈ ਇਸਤਰੀ ਨਹੀਂ ਹੋਈ ਹੈ ॥੩੨॥",
              "additional_information": {}
            }
          }
        }
      },
