        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Rani you are praiseworthy, after winning the war you have saved me,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਰਾਨੀ! ਤੂੰ ਧੰਨ ਹੈ ਜਿਸ ਨੇ ਰਣ ਨੂੰ ਜਿਤ ਕੇ ਮੈਨੂੰ ਬਚਾ ਲਿਆ ਹੈ।",
              "additional_information": {}
