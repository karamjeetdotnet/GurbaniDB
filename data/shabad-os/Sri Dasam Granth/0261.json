        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "These sword are the destroyers of the enemies and appear like saws",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਦੀ ਨੇਕ ਸੂਏ ਵਾਂਗ ਸ਼ੋਭਦੀ ਹੈ। ਵੱਡੀ ਛਬੀ ਨੂੰ ਧਾਰਦੀ ਹੈ, ਚੰਗੇ ਉਪਾਅ ਬਣਾਉਣ ਵਾਲੀ ਹੈ ਅਤੇ ਵੈਰੀ ਨੂੰ ਗਾਲਣ ਵਾਲੀ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
