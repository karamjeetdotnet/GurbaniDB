        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Or else she will be too firm and will definitely marry me.’(6)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦ ਇਹ ਮੈਨੂੰ ਅਜ ਹੀ ਭਾਵੇਂ ਵਰ ਲਏ ॥੬॥",
              "additional_information": {}
            }
          }
        }
      }
