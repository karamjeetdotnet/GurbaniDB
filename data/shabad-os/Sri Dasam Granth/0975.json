        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੇ (ਇਹ) ਕਠਿਨ ਕੰਮ ਇਹ ਇਸਤਰੀ ਕਰ ਲਏ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "W8JL",
        "source_page": 975,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
