          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦ (ਉਸ) ਸੁੰਦਰ ਆਦਮੀ ਨੇ ਮਨ ਵਿਚ ਵਿਚਾਰ ਕੀਤਾ",
              "additional_information": {}
            }
          }
        }
      },
