          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਧਰੋਂ ਸੂਰਮੇ ਗਜੇ ਅਤੇ ਭਿਆਨਕ ਨਾਦ ਹੋਣ ਲਗੇ।",
              "additional_information": {}
            }
          }
        }
      },
