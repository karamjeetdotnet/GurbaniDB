        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Some of them fled away from the city, but came back on account of saarevation.1.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਈ ਭੁਖੇ ਮਰਦਿਆਂ ਪਰਤ ਆਏ ॥੧॥",
              "additional_information": {}
            }
          }
        }
      },
      {
