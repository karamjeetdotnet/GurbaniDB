        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Once she came across a handsome man, and fell in love with him.(3)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਰਾਣੀ ਨੇ ਉਸ ਨਾਲ ਪ੍ਰੇਮ ਲਗਾ ਲਿਆ ॥੩॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "FL7C",
        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "AiDk pRIiq rwnI ko lwgI ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਣੀ (ਉਸ ਦੇ) ਪ੍ਰੇਮ ਵਿਚ ਬਹੁਤ ਲਗ ਗਈ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "6GTF",
        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
