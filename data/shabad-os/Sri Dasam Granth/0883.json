        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "What did Rani do and said, no body could perceive the circumstances.(7)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਉਸ ਨੇ ਕੀ ਕੀਤਾ ਅਤੇ ਕੀ ਕਿਹਾ ॥੭॥",
              "additional_information": {}
            }
          }
        }
      }
