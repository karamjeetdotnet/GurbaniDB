          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇੰਦ੍ਰ ਮਤੀ ਨੂੰ ਗਰਭ ਠਹਿਰ ਗਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
