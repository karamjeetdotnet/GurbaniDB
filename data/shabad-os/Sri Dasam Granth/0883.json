        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "Xo kih iPir AweI Gr mwhI ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Reprimanding her not to divulge this to anyone, she returned.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਹ ਕਹਿ ਕੇ ਫਿਰ ਘਰ ਪਰਤ ਆਈ।",
              "additional_information": {}
            }
          }
        }
      },
