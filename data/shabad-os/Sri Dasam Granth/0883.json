        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "qw ko Dwm drbu so BirXo ]6]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She left him at the house of a lady-friend and gave her lot of wealth.(6)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਉਸ ਦਾ ਘਰ ਧਨ ਨਾਲ ਭਰ ਦਿੱਤਾ ॥੬॥",
              "additional_information": {}
            }
          }
        }
      },
