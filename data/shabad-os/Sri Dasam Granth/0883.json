        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She pretended to be suffering from stomach malady, and no man could discern the mystery.(5)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਕੋਈ ਵਿਅਕਤੀ ਵੀ ਭੇਦ ਨੂੰ ਸਮਝ ਨਾ ਸਕਿਆ ॥੫॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "4139",
        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
