        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She pretended to be suffering from stomach malady, and no man making love.(4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਦੋਹਾਂ ਨੇ ਮਿਲ ਕੇ ਬਹੁਤ ਕਾਮ-ਕ੍ਰੀੜਾ ਕੀਤੀ ॥੪॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "K707",
        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "rhq bhuq idn qw sO BXo ]"
