        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After nine months, she gave birth to a son, who looked like the Cupid.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਇੰਜ ਪ੍ਰਤੀਤ ਹੁੰਦਾ ਸੀ) ਮਾਨੋ ਕਾਮ ਦੇਵ ਆਪ ਹੀ ਬਣਿਆ ਹੋਵੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "QQP8",
        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "eyk nwir ky Gr mY DirXo ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਨੂੰ) ਇਕ ਇਸਤਰੀ ਦੇ ਘਰ ਟਿਕਾ ਦਿੱਤਾ",
              "additional_information": {}
            }
          }
        }
      },
      {
