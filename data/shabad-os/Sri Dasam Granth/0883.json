        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨੌਂ ਮਹੀਨੇ ਬੀਤਣ ਤੇ (ਇਕ) ਪੁੱਤਰ ਨੂੰ ਜਨਮ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "PPZH",
        "source_page": 883,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
