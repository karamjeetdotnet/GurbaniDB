        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦੂਜੇ ਕੰਨ ਕਿਸੇ ਨੂੰ ਖ਼ਬਰ ਨਾ ਹੋਈ",
              "additional_information": {}
            }
          }
        }
      },
