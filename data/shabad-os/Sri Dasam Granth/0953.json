        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(They thought,) ‘After losing Raja, how can we go home and how",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਜੇ ਨੂੰ ਦੇ ਕੇ (ਅਰਥਾਤ ਮਰਵਾ ਕੇ) ਅਸੀਂ ਘਰ ਕੀ ਜਾਵਾਂਗੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "KB5M",
        "source_page": 953,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "khw rwinXih bkqR idKY hY ]37]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "shall we show our faces to the Rani?’(37)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਰਾਣੀ ਨੂੰ ਕੀ ਮੂੰਹ ਵਿਖਾਵਾਂਗੇ ॥੩੭॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "0K1G",
        "source_page": 953,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
