        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The servants ran forward and took him in their laps.(36)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨੌਕਰਾਂ (ਸੇਵਕਾਂ) ਨੇ ਕੋਲ ਪਹੁੰਚ ਕੇ ਉਸ ਨੂੰ ਗਲੇ ਨਾਲ ਲਗਾ ਲਿਆ ॥੩੬॥",
              "additional_information": {}
            }
          }
        }
      }
