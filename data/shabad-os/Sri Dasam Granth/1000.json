          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦੋਹਰਾ:",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "04P0",
        "source_page": 1000,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "rwjw kO kir bis ilXo; dIno jwr inkwir ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After getting the paramour to escape, she had won over the Raja,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੇ ਰਾਜੇ ਨੂੰ ਵਸ ਵਿਚ ਕਰ ਲਿਆ ਅਤੇ ਯਾਰ ਨੂੰ ਕੱਢ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "9F37",
        "source_page": 1000,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "siKXn mY swcI BeI; qOnY sKI sMGwir ]33]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "And by killing the maid, she established her honesty as well.(33)(1)",
              "additional_information": {}
            }
          },
