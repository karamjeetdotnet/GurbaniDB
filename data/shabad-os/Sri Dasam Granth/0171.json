        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਰਾਜੇ ਨੇ) ਬ੍ਰਾਹਮਣ ਨੂੰ (ਆਪਣੇ ਕੋਲ) ਬੁਲਾ ਲਿਆ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "X828",
        "source_page": 171,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
