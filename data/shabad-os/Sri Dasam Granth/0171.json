        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "This Brahmin recited all the four Vedas, which the king listened to attentively.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਜੇ ਨੂੰ ਸੁਣਾਇਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "SZ9Q",
        "source_page": 171,
        "source_line": null,
