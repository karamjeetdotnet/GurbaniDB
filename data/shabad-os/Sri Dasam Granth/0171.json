        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The king quaffed the water, with which the feet of the Brahmin had been washed and offered charities.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੇ ਅਨੇਕ ਵਾਰ ਪ੍ਰਦਖਣਾ ਕੀਤੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "40ET",
        "source_page": 171,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "kroir dCnw deI ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਫਿਰ) ਕਰੋੜਾਂ ਦੱਛਣਾ ਦਿੱਤੀਆਂ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "AWLL",
        "source_page": 171,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
