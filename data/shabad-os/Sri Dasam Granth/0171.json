          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਬ੍ਰਾਹਮਣ ਨੇ ਇਸ ਤਰ੍ਹਾਂ ਗੱਲ ਕਹੀ,",
              "additional_information": {}
            }
          }
        }
      },
