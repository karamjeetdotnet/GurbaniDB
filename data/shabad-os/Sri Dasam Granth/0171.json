        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਵੇਂ ਜਿਵੇਂ ਰਾਜਾ ਪ੍ਰਿਥਵੀ ਦੇਣ ਦੀ ਗੱਲ ਕਹਿੰਦਾ,",
              "additional_information": {}
            }
          }
        }
      },
