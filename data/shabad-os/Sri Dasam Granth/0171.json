        "source_page": 171,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "iqmu iqmu nwih puroihq ghY ]9]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "As many times the king orders for the donation of the earth, for so many times the preceptor Shukracharya asks him not to agree to it.9.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਿਵੇਂ ਤਿਵੇਂ ਪੁਰੋਹਿਤ ਨਾਂਹ ਨਾਂਹ ਕਹੀ ਜਾਂਦਾ ॥੯॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "RJYE",
        "source_page": 171,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "jb inRp dyn Drw mnu kInw ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਦੋਂ ਰਾਜੇ ਨੇ ਧਰਤੀ ਦੇਣ ਦਾ ਮਨ ਬਣਾਇਆ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "PPW2",
        "source_page": 171,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
