        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The great warriors, getting enraged, have attacked and are waging a dreadful and frightening war",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਥੇ ਮਹਾ ਭਿਆਨਕ ਅਤੇ ਡਰਾਵਣਾ ਯੁੱਧ ਮੰਡਿਆ ਹੋਇਆ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
