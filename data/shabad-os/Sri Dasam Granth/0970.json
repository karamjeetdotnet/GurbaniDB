        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She asked their seven daughters to walk ahead and picked three sons in her lap.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਨ੍ਹਾਂ ਨੇ ਸੱਤ ਧੀਆਂ ਨੂੰ ਅਗੇ ਕਰ ਲਿਆ ਅਤੇ ਤਿੰਨ ਪੁੱਤਰਾਂ ਨੂੰ ਇਸਤਰੀ ਨੇ ਸੰਭਾਲ ਲਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "3V5J",
        "source_page": 970,
        "source_line": null,
