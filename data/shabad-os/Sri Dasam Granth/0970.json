        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "They all felt blissful and none, neither the old nor the young, remained behind.(24)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਬਿਰਧ, ਬਾਲ, ਜਵਾਨ ਕੋਈ ਵੀ ਘਰ ਨਹੀਂ ਰਿਹਾ ॥੨੪॥",
              "additional_information": {}
            }
          }
        }
      },
      {
