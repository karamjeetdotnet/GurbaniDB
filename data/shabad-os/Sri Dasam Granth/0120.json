        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The brave fighters were roaring in the battlefield.7.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੈਦਾਨੇ ਜੰਗ ਵਿਚ ਯੋਧੇ ਗੱਜ ਰਹੇ ਸਨ ॥੭॥",
              "additional_information": {}
