        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When the Shah’s wife heard about him,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਹ (ਗੱਲ) ਸ਼ਾਹ ਦੀ ਵਹੁਟੀ ਨੇ ਸੁਣ ਲਈ।",
              "additional_information": {}
            }
          }
        }
      },
