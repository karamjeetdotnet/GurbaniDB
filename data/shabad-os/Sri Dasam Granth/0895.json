        "source_page": 895,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "Gwt gVn ihq qwih bulwXo ]1]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She called him to get the ornaments made.(1)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ (ਗਹਿਣਿਆਂ ਦੀ) ਘਾੜਤ ਘੜਨ ਲਈ ਬੁਲਾ ਲਿਆ ॥੧॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
  },
