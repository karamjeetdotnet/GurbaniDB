        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He is the Motivator of gods and destroyer of all. 1. 267;",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਜਾਨਬਾਹੁ ਅਤੇ ਹੋਰ ਸਾਰਿਆਂ ਨੂੰ ਮਿਧਣ ਵਾਲਾ ਹੈਂ ॥੧॥੨੬੭॥",
              "additional_information": {}
            }
          }
        }
      },
