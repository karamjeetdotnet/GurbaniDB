        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After a good thrashing by Krishna, all the remaining army of the kings ran away",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕ੍ਰਿਸ਼ਨ ਕੋਲੋਂ ਬਹੁਤ ਮਾਰ ਖਾ ਕੇ ਰਾਜਿਆਂ ਦੀ ਸੈਨਾ ਭਜ ਗਈ।",
              "additional_information": {}
            }
          }
        }
