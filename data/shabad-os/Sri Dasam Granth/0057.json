          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਵੇਂ ਪ੍ਰਭੂ ਨੇ ਕਿਹਾ ਹੈ, ਉਸੇ ਤਰ੍ਹਾਂ ਜਗਤ ਨੂੰ ਕਹਾਂਗਾ;",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "5HT5",
        "source_page": 58,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
