        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Whatever the Lord said, I am repeating the same unto you, I do not bear enmity with anyone.31.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੋਰ ਕਿਸੇ ਨਾਲ ਵੈਰ ਨਹੀਂ ਪਾਲਾਂਗਾ ॥੩੧॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "EXGR",
        "source_page": 58,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "jo hm ko prmysr aucirhYN ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੋ ਲੋਕ ਸਾਨੂੰ ਪਰਮੇਸ਼ਵਰ ਕਹਿਣਗੇ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "VWAD",
        "source_page": 58,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "SGPC": "qy sB nrk kuMf mih pirhYN ]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Whosoever shall call me the Lord, shall fall into hell.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹ ਸਾਰੇ ਨਰਕ-ਕੁੰਡ ਵਿਚ ਪੈਣਗੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "Q900",
        "source_page": 58,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
