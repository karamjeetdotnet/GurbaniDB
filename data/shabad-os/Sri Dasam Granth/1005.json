        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I am the daughter of Farangh Shah and the demon carries me away(to Quazi).",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਉਸ ਵਿਚ ਲਿਖਿਆ ਸੀ) ਮੈਂ ਫਿਰੰਗ ਰਾਜੇ ਦੀ ਧੀ ਹਾਂ ਅਤੇ ਮੈਨੂੰ ਦੇਓ ਉਠਾ ਲਿਆਉਂਦਾ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "NN2Q",
        "source_page": 1005,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "mo so kwjI mwin riq; dyh qhw phucwie ]17]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘When Quazi had made love to me, he sends me back.(l7)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੇਰੇ ਨਾਲ ਕਾਜ਼ੀ ਕਾਮ-ਕ੍ਰੀੜਾ ਕਰਦਾ ਹੈ ਅਤੇ (ਫਿਰ) ਮੈਨੂੰ ਉਥੇ ਪਹੁੰਚਾ ਦਿੰਦਾ ਹੈ ॥੧੭॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "DXM5",
        "source_page": 1005,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
