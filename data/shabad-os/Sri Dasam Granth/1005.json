        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I have fallen in love with you, and that is why I am writing this letter.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੈਂ ਤੈਨੂੰ (ਵੇਖਦਿਆਂ ਹੀ) ਮੋਹਿਤ ਹੋ ਗਈ ਹਾਂ, ਇਸ ਲਈ ਇਹ ਪੱਤਰ ਲਿਖਿਆ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
