        "id": "GTFY",
        "source_page": 1005,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "bhuro Kwt auTwie kY dIjhu hÎW phucwie ]14]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘And after that pick up her bed and bring her back here.’(14)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਫਿਰ ਮੰਜੀ ਨੂੰ ਚੁਕ ਕੇ ਇਥੇ ਪਹੁੰਚਾ ਦਿਓ ॥੧੪॥",
