        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "“I was the son of a Kshatriya and was filled with ire, therefore I destroyed him",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੈਂ ਛਤ੍ਰੀ ਦਾ ਪੁੱਤਰ ਸਾਂ, ਕ੍ਰੋਧ ਨਾਲ ਭਰ ਕੇ ਉਸ ਨੂੰ ਨਸ਼ਟ ਕੀਤਾ ਹੈ; ਮੇਰੀ ਬੇਨਤੀ ਸੁਣ ਲਵੋ।",
              "additional_information": {}
            }
          }
        }
      },
