        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He then performed austerities for a long time, by which the Immanent Lord became merciful towards him",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਫਿਰ 'ਕਾਲ-ਪੁਰਖ' (ਉਸ ਉੱਤੇ) ਕ੍ਰਿਪਾਲੂ ਹੋਏ।",
              "additional_information": {}
            }
          }
        }
      },
