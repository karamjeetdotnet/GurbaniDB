        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Because of this event, he felt extremely ashamed and his pride was extremely shattered.14.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ (ਉਸ ਦੇ ਮਨ ਵਿਚੋਂ) ਵੱਡਾ ਹੰਕਾਰ ਦੁਰ ਹੋ ਗਿਆ ॥੧੪॥",
              "additional_information": {}
            }
          }
        }
      },
