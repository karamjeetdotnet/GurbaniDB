        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He also abandoned the meditation of any other, therefore he was also blemished.12.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸੇ ਕਰਕੇ ਉਸ ਨੂੰ ਕਲੰਕ ਲੱਗ ਗਿਆ ॥੧੨॥",
              "additional_information": {}
            }
          }
        }
      },
      {
