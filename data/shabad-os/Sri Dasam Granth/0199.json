          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬ੍ਰਹਸਪਤੀ (ਅੰਬਰ) ਦੀ ਇਸਤਰੀ ਨਾਲ (ਚੰਦ੍ਰਮਾ ਨੇ) ਭੋਗ ਕੀਤਾ ਸੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "FN89",
        "source_page": 199,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
