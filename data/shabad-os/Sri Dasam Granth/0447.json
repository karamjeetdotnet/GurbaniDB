        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He reflected how the Lord Krishna had remembered him?1499.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਹ ਵਿਚਾਰ ਮਨ ਵਿਚ ਬਣਾ ਕੇ ਕਿ ਸ੍ਰੀ ਕ੍ਰਿਸ਼ਨ ਨੇ ਮੈਨੂੰ ਯਾਦ ਕੀਤਾ ਹੈ ॥੧੪੯੯॥",
              "additional_information": {}
            }
          }
        }
      },
