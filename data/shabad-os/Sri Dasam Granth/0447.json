          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਯੁੱਧ-ਭੂਮੀ ਨੂੰ ਵੇਖ ਕੇ (ਸ਼ਿਵ ਨੇ) ਮਨ ਵਿਚ ਕਿਹਾ ਕਿ ਇਹ ਤਾਂ ਚਿੰਤਾ ਦਾ ਮੌਕਾ ਹੈ ॥੧੫੦੨॥",
              "additional_information": {}
            }
          }
