          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੋਰ ਵੀ ਸਾਰੇ ਗਣ ਆਪਣੇ ਆਪਣੇ ਸ਼ਸਤ੍ਰ ਲੈ ਕੇ ਚਲ ਪਏ",
              "additional_information": {}
            }
          }
        }
