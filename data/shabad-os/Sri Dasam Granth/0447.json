          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਗਣੇਸ਼ ('ਏਕ ਰਦਨ') ਵੀ ਸਾਰੀ ਫੌਜ ਲੈ ਕੇ ਚਲ ਪਿਆ।",
              "additional_information": {}
            }
          }
        }
