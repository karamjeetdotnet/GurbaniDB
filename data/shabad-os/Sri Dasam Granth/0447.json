          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦੋਂ ਸ਼ਿਵ ਦੇ ਨਾਲ ਸ਼ੂਰਵੀਰ ਸ਼ਿਵ-ਗਣ ਉਠ ਕੇ ਤੁਰ ਪਏ।",
              "additional_information": {}
            }
          }
        }
