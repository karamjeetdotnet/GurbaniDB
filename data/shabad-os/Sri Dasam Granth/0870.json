          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸੁਭਟ ਸਿੰਘ ਨੇ ਇਸ ਤਰ੍ਹਾਂ ਕਿਹਾ ਕਿ ਮੇਰੇ ਪਾਸ ਅਗੇ ਇਕ ਇਸਤਰੀ ਹੈ।",
              "additional_information": {}
            }
          }
        }
