        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Therefore, even insisted upon, I will not marry second time.’(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਇਸ ਲਈ ਮੈਂ ਇਸ ਨਾਲ) ਦੂਜਾ ਵਿਆਹ ਨਹੀਂ ਕਰਾਂਗਾ ਜੇ ਮੈਨੂੰ ਅਨੇਕ ਲੋਕ ਕਿਉਂ ਨਾ ਕਹਿਣ ॥੧੦॥",
              "additional_information": {}
            }
          }
        }
      }
