          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸੇ ਤਰ੍ਹਾਂ ਦਕਸ਼ ਦਾ ਸਿਰ ਤ੍ਰਿਸ਼ੂਲ ਨਾਲ ਕੱਟੇ ਜਾਣ ਤੇ ਡਿਗ ਪਿਆ।",
              "additional_information": {}
            }
          }
        }
