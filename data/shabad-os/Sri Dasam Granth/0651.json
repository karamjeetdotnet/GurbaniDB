        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When, passing through the forests, cities and mountains, they went forward, there they saw a trader coming.200.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦ ਉਸ ਨੇ ਇਕ ਬਨਜਾਰਾ ਆਉਂਦਾ ਹੋਇਆ ਵੇਖਿਆ ॥੨੦੦॥",
              "additional_information": {}
            }
          }
        }
