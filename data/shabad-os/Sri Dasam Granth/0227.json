        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "jugIAw ko kIno bys kausl ky Cor dys rwjw rwmcMd jU ky sMig hI isDwir hON ]266]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "I shall adopt the garb of a yogi, and leaving Kaushal (country), I shall go with the king Ram.266.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਯੋਗੀਆਂ ਵਾਲਾ ਭੇਸ ਕਰਕੇ ਕੌਸ਼ਲ ਦੇਸ਼ ਦੇ ਕਲੇਸ਼ਾਂ ਨੂੰ ਛੱਡ ਕੇ ਰਾਜਾ ਰਾਮ ਚੰਦਰ ਜੀ ਦੇ ਨਾਲ ਬਣ ਨੂੰ ਜਾਵਾਂਗੀ ॥੨੬੬॥",
              "additional_information": {}
            }
          }
        }
      }
