        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The blows were struck with tridents, swords, arrows and other weapons and arms and the warriors swung and fell",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤ੍ਰਿਸ਼ੂਲ, ਸੈਹੱਥੀ, ਪਰਘ, ਪੱਟਾ ਅਤੇ ਬਾਣ ਆਦਿ ਦੇ ਪ੍ਰਹਾਰ ਹੋਣ ਲਗੇ।",
              "additional_information": {}
            }
          }
        }
      },
