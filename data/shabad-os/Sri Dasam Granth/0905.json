        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
          "Budha Dal Mehron": "bMDu iBRq rodn krY; rwj bMs qy jwie ]35]"
        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘The servants and the relatives are weeping, who will now rule over?’(35)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਭਰਾ ਭਾਈ ਅਤੇ ਨੌਕਰ ਰੋ ਰਹੇ ਹਨ ਕਿ ਬੰਸ ਤੋਂ ਰਾਜ ਜਾ ਰਿਹਾ ਹੈ ॥੩੫॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
