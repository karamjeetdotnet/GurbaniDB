          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੁਸੀਂ ਪੁੱਤਰ, ਬਾਲਕ, ਨੌਜਵਾਨ ਇਸਤਰੀ ਅਤੇ ਠਾਠ ਬਾਠ ਛਡ ਰਹੇ ਹੋ।",
              "additional_information": {}
            }
          }
        }
      },
