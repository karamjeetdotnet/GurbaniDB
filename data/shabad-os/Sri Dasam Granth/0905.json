        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "I will tell you through the wisdom of Vedas.(39)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੋ ਕੁਝ ਸ਼ਾਸਤ੍ਰਾਂ, ਸਮ੍ਰਿਤੀਆਂ ਅਤੇ ਵੇਦ ਦਸਦੇ ਹਨ ॥੩੯॥",
              "additional_information": {}
            }
          }
