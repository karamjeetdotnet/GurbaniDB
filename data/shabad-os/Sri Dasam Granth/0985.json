          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਬਹੁਤ ਸਾਰੇ ਰਾਜਿਆਂ ਨੇ ਦਲ ਬਣਾ ਕੇ ਵੈਰੀ ਨੂੰ ਜਿਤਣ ਲਈ ਮਨ ਵਿਚ ਵਿਉਂਤ ਬਣਾਈ ਅਤੇ (ਵੈਰੀ ਉਤੇ) ਚੜ੍ਹ ਚਲੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JAXX",
        "source_page": 985,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
