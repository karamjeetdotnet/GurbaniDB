          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਲੰਕਾ ਵਿਚ ਇਕ ਵਿਗੜੇ ਹੋਏ ਦੈਂਤ ਨੇ ਰਘੁਨੰਦਨ (ਸ੍ਰੀ ਰਾਮ) ਦੀ ਕਹਾਣੀ ਸੁਣੀ",
              "additional_information": {}
            }
          }
        }
