        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "On this side, the chastity of woman was defiled and on that side all the army was chopped. By this the pride of Jalandhar was shattered.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਜਲੰਧਰ ਰਾਜੇ ਦੀ ਦੇਹ ਦਾ ਸਾਰਾ ਹੰਕਾਰ ਮਿਟ ਗਿਆ।",
              "additional_information": {}
            }
          }
        }
