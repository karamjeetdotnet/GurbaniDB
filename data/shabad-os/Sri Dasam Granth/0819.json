        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "In the mean time the constables from the City Kotwal, the police station officer, walked in.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਪਿਛੋਂ ਕੋਤਵਾਲ ਦੇ ਪਿਆਦੇ ਆ ਗਏ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "YSS3",
        "source_page": 819,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
