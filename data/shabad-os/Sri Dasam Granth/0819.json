        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She fell in love with the Raja too and through her maid,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹ ਵੀ ਰਾਜੇ ਪ੍ਰਤਿ ਮੋਹਿਤ ਹੋ ਗਈ ਅਤੇ ਰੋਜ਼ ਉਸ ਨੂੰ ਘਰ ਸਦ ਲੈਂਦੀ।",
              "additional_information": {}
            }
          }
        }
      },
