        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Chitarkala, called the Raja to her house.(4)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚਿਤ੍ਰਕਲਾ ਨਾਂ ਦੀ ਇਕ ਸਖੀ ਨੂੰ ਉਸ ਦੇ ਘਰ ਭੇਜ ਦਿੰਦੀ ॥੪॥",
              "additional_information": {}
            }
          }
        }
      },
