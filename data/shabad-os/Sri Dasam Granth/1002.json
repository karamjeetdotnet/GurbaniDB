        },
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After a few hours, when he regained full consciousness, the servantsspoke in sycophantic tones.(23)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨੌਕਰਾਂ ਚਾਕਰਾਂ ਨੇ ਬਹੁਤ ਤਰ੍ਹਾਂ ਨਾਲ ਵਡਿਆਈ ਕੀਤੀ ॥੨੩॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "ZAGD",
        "source_page": 1002,
        "source_line": null,
        "pronunciation": null,
        "pronunciation_information": null,
        "type": "Pankti",
        "gurmukhi": {
