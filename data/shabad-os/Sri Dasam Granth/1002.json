          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਘੁਮੇਰੀ ਖਾ ਕੇ ਧਰਤੀ ਉਤੇ ਡਿਗ ਪਿਆ ਅਤੇ ਅੱਖਾਂ ਨਾ ਖੋਲ੍ਹੀਆਂ ॥੨੧॥",
              "additional_information": {}
            }
          }
        }
      },
